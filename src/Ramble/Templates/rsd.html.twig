<?xml version="1.0" ?>
<rsd version="1.0" xmlns="http://archipelago.phrasewise.com/rsd" >
    <service>
        <engineName>Ramble Blog</engineName>
        <homePageLink>{{ base_url() }}</homePageLink>
        <apis>
            <api name="MetaWeblog" preferred="false" apiLink="{{ base_url() ~ path_for('XMLRPC') }}" blogID="1" />
            <api name="Blogger" preferred="false" apiLink="{{ base_url() ~ path_for('XMLRPC') }}" blogID="1" />
            <api name="WordPress" preferred="true" apiLink="{{ base_url() ~ path_for('XMLRPC') }}" blogID="1" />
        </apis>
    </service>
</rsd>
